<!--
  Fonts
-->
<link href="https://fonts.googleapis.com/css?family=Audiowide|Roboto+Mono|Teko" rel="stylesheet">

<!--
mobile styles
-->
<link rel="stylesheet" type="text/css" href="css/style.css">


  </head>

<body>
  <div class="dropdown">
      <button class="btn btn-success dropbtn">Menu</button>
        <div class="dropdown-content">
          <ul class='list-group'>
            <li class='list-group-item'><%= link_to "Go Back to My Profile", root_path, id: "home" %></li>
            <li class='list-group-item'><%= link_to "Check out All of Todays Achievements!", achievements_path, class: "all-achievements" %></li>
            <li class='list-group-item'><%= link_to "Check On your Fellow Adventurers!", users_path %></li>
          </ul>
        </div>
      </div></li>

    <%= yield %>
</body>
</html>
